{
  "name": "whereami",
  "version": "0.1.0",
  "description": "Idea based off of geoguessr. This was a weekend project I did just to come up with a first version. Feel free to pull, and add PRs on bugs you find (I know there are quite a few, which I'll get around to fiddling with on rainy days!).",
  "main": "bundle.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && set NODE_ENV=production && webpack -p --config webpack.config.babel.js",
    "start": "npm run clean && set NODE_ENV=dev && webpack-dev-server --colors --hot --progress",
    "test": "jest -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willdavidow/whereami.git"
  },
  "contributors": [
    {
      "name": "Brian Kinney",
      "email": "webdevbrian@gmail.com",
      "url": ""
    },
    {
      "name": "Will Davidow",
      "email": "w.davidow@gmail.com",
      "url": "davidow.io"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdevbrian/whereami/issues"
  },
  "homepage": "https://github.com/webdevbrian/whereami#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "breakpoint-sass": "^2.7.1",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-google-maps": "^7.1.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.2",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.6",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "bail": false,
    "collectCoverage": true,
    "resetModules": true,
    "verbose": true,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(jsx|js)$",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ]
  }
}
